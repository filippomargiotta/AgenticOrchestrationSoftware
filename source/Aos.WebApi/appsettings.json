{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "EventLog": {
    "Directory": "data",
    "FileName": "eventlog.jsonl"
  },
  "HelloWorkflow": {
    "Models": [
      {
        "ModelId": "openai-gpt-4.1-mini",
        "Provider": "openai",
        "Version": "2026-02"
      }
    ],
    "Tools": [
      {
        "ToolId": "web-search",
        "Version": "1.0"
      }
    ],
    "PolicyDecisions": [
      {
        "PolicyId": "allow-approved-tools",
        "Decision": "allow",
        "Reason": "configured default policy"
      }
    ]
  },
  "AllowedHosts": "*"
}
